<script src="@Url.Content("~/Content/plugins/datepicker/bootstrap-datepicker.js")"></script>
<script>
    $(function () {



        var date = new Date();
        var d = new Date();
        d.setDate(date.getDate() - 0);

        var sd = new Date();
        sd.setDate(date.getDate() - 180);



        //Date picker
        $('#DateFrom').datepicker({
            autoclose: true
            , dateFormat: 'yyyy-mm-dd',
            endDate: d,
            startDate: sd
        });

        $('#DateTo').datepicker({
            autoclose: true
            , dateFormat: 'yyyy-mm-dd',
            endDate: d,
            startDate: sd
        });


    });
</script>

