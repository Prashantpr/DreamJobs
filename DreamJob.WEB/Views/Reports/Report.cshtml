    function GetExamDetail(ApplicantID, AttemptID) {
        
       
        $('#ExamDetail').html('Please wait...');
        $('#ResultSummary').html('Loading result');
        //$('[name="btnShow"]').attr({ 'value': 'Please wait' });
        $.ajax({
            url: "@Url.Action("GetExamDetail", "Reports")",
