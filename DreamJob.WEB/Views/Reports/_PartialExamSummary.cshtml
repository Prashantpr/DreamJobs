

@using (Html.BeginForm("ExamSummary", "Reports", FormMethod.Post, new { id = "frmHistory" }))
{

    <div class="row">
        <div class="col-md-12 col-lg-12">
            <div class="card">
                <div class="card-header">
                    
                    <div class="row">
                        <div class="col-md-1">Job Type</div>
                        <div class="col-md-2">
                            @Html.DropDownList("ddlJobType", (SelectList)ViewBag.Jobs, "All  Job Type", new {@name="JobID",  @class = "form-control select2", style = "width:100%", data_placeholder = "All Job Type" })
                        </div>
                        <div class="col-1">Start Date</div>
                        <div class="col-2"><input class="form-control" placeholder="(mm/dd/yyyy)" title="Exam Date From" id="DateFrom" name="DateFrom" /></div>
                        <div class="col-1">End Date</div>
                        <div class="col-2"><input class="form-control" placeholder="(mm/dd/yyyy)" title="To" id="DateTo" name="DateTo" /></div>
                        <div class="col-md-3">
                            <input type="button" name="btnShow" onclick="ShowExamSummary();" value="Show Summary" class="btn btn-primary" />
                            @*<input type="button" onclick="exportexcel();" value="Export to excel" class="btn btn-warning" />*@
                        </div>
                        </div>
                    </div>
                <div class="card-body">
                    <div id="reportData"></div>
                </div>
            </div>
        </div>
    </div>

} 
